#Aggregate Query For Unique Company, Domain, App Totals, and Categories
db.constants.aggregate([
  { $group: { _id:"$companyName", domain:{$first:"$domain"}, appsTotal: { $sum: 1 }, categories:{$addToSet:"$category"}}},
  {$sort:{appsTotal:-1}}]);



